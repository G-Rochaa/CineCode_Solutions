<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="Control.ControllerFilmesXSala">
	<center>
		<VBox styleClass="vbox-center-mae" fx:id="ControllerFormsSessao">
			<!-- Horário de Início -->
			<HBox spacing="10">
				<Label text="Horário de Início:" styleClass="label-editar" />
				<TextField fx:id="inputHorarioInicio" styleClass="input-label" />
			</HBox>

			<!-- Horário que Finaliza -->
			<HBox spacing="10">
				<Label text="Horário que Finaliza:" styleClass="label-editar" />
				<TextField fx:id="inputHorarioFim" styleClass="input-label" />
			</HBox>
		</VBox>
	</center>
</BorderPane>